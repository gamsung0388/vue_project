<mapper namespace="BoardMapper">
  	<insert id="insert" parameterType="BoardDTO">
  		Insert board(
			BOARD_NUM
		    ,BOARD_TITLE
		    ,BOARD_TXT
		    ,BOARD_TAG
		    ,USER_ID
		    )values(
				#{boardNum}
		        ,#{boardTitle}
		        ,#{boardTxt}
		        ,#{boardTag}
		        ,#{userId}
			);
  	
  	</insert>
	
</mapper>  